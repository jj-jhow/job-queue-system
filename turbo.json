{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "outputs": [
                "dist/**",
                "build/**"
            ],
            "dependsOn": [
                "^build"
            ]
        },
        "test": {
            "dependsOn": [
                "^build"
            ]
        },
        "lint": {},
        "dev": {
            "cache": false,
            "persistent": true,
            "dependsOn": [
                "^build"
            ]
        },
        "start": {
            "cache": false,
            "persistent": true,
            "dependsOn": [
                "^build"
            ]
        }
    }
}